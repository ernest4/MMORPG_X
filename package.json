{
  "name": "game_prototype_network",
  "version": "0.1.0",
  "description": "isomorphic game server",
  "main": "dist/src/server/index.js",
  "scripts": {
    "test": "jest --config ./jest.config.js",
    "start": "node dist/server/index.js",
    "build": "yarn build-css && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"yarn build-server\" \"yarn build-client\"",
    "build-css": "postcss src/client/css/tailwind.css -o src/client/css/main.css",
    "build-server": "tsc -b src/server",
    "build-client": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack --mode production"
  },
  "author": "Ernestas Monkevicius",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@types/jest": "^26.0.19",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/reduce-reducers": "^1.0.0",
    "@types/redux-actions": "^2.6.1",
    "autoprefixer": "^10.2.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^6.3.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^5.0.1",
    "cssnano": "^5.0.8",
    "html-webpack-plugin": "^4.5.0",
    "immer": "^6.0.3",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "mini-css-extract-plugin": "^1.3.4",
    "phaser": "^3.52.0",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^4.2.0",
    "postcss-preset-env": "^6.7.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "tailwindcss": "^2.0.2",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "uWebSockets.js": "uNetworking/uWebSockets.js#v20.0.0"
  }
}
